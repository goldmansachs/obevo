<license-lookup xmlns="https://source.jasig.org/schemas/maven-notice-plugin/license-lookup"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://source.jasig.org/schemas/maven-notice-plugin/license-lookup https://source.jasig.org/schemas/maven-notice-plugin/license-lookup/license-lookup-v1.0.xsd">
    <!-- These artifact licenses could not be resolved by default; needed license entries as defined in
    https://source.jasig.org/licenses/license-mappings.xml -->
    <artifact>
        <groupId>junit-addons</groupId>
        <artifactId>junit-addons</artifactId>
        <license>Apache License, Version 2.0</license>
    </artifact>
    <artifact>
        <groupId>com.h2database</groupId>
        <artifactId>h2</artifactId>
        <license>EPL 1.0</license>
    </artifact>

    <!-- Overriding display name (config is rather unclear on its own in the NOTICE.txt file) -->
    <artifact>
        <groupId>com.typesafe</groupId>
        <artifactId>config</artifactId>
        <version type="regex">.*</version>
        <name>Typesafe Config</name>
    </artifact>

    <!-- Picking a specific license for a multi-licensed product -->
    <artifact>
        <groupId>org.eclipse.collections</groupId>
        <artifactId>eclipse-collections</artifactId>
        <version type="regex">.*</version>
        <license>Eclipse Public License - v 1.0</license>
    </artifact>
    <artifact>
        <groupId>org.eclipse.collections</groupId>
        <artifactId>eclipse-collections-testutils</artifactId>
        <version type="regex">.*</version>
        <license>Eclipse Public License - v 1.0</license>
    </artifact>
    <artifact>
        <groupId>org.eclipse.collections</groupId>
        <artifactId>eclipse-collections-api</artifactId>
        <version type="regex">.*</version>
        <license>Eclipse Public License - v 1.0</license>
    </artifact>
    <artifact>
        <groupId>org.jgrapht</groupId>
        <artifactId>jgrapht-core</artifactId>
        <version type="regex">.*</version>
        <license>Eclipse Public License (EPL) 1.0</license>
    </artifact>
    <artifact>
        <groupId>us.fatehi</groupId>
        <artifactId>schemacrawler</artifactId>
        <version type="regex">.*</version>
        <license>Eclipse Public License - v 1.0</license>
    </artifact>
